---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" version="2.0">
  <channel>
    <title>audio blog</title>
    <link>http://davisland.info/blog</link>
    <language>en-us</language>
    <itunes:subtitle>Here's some stuff I've made.</itunes:subtitle>
    <itunes:author>Davis Land</itunes:author>
    <itunes:summary>An audio blog of things I've made.</itunes:summary>
    <itunes:owner>
        <itunes:name>Davis Land</itunes:name>
        <itunes:email>dland95@gmail.com</itunes:email>
    </itunes:owner>
    <itunes:image href="http://davisland.info/podcast_art.png"/>
    <itunes:category text="Arts" />
    <itunes:explicit>no</itunes:explicit>
    <itunes:copyright text="Davis Land" />
    <link>{{ site.url }}</link>
    {% for post in site.categories.podcast %}
        <item>
          <title>{{ post.title | xml_escape }}</title>
          <itunes:author>{{ post.author }}</itunes:author>
          <itunes:summary>{{ post.summary }} {{ post.bumper }}</itunes:summary>
          <itunes:image href="http://davisland.info/podcast_art.png"/>
          <enclosure url = "{{ post.enclosure }}" length="{{ post.length }}" type="{{ post.type }}"></enclosure>
          <pubDate>{{ post.date | date: "%a, %d %b" }}</pubDate>
          <itunes:duration>{{ post.duration }}</itunes:duration>
        </item>
    {% endfor %}
  </channel>
</rss>

